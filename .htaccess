<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteBase /sisclimatico.herokuapp.com
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule ^(.*)$ index.php/$1 [L]
</IfModule>
<IfModule !mod_rewrite.c>
    ErrorDocument 404 /sisclimatico.herokuapp.com/index.php
</IfModule>
